{
    "manifest_version": 2,
    "name": "Video sites @RKV",
    "version": "0.1",
    "description": "This extension load the video related websites when title or meta discription as video ",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "video-camera.png",
        "default_title": "Load Video sites @RKV"
    },
   "permissions": ["activeTab"]
}